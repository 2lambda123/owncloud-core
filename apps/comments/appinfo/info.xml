<?xml version="1.0"?>
<info>
	<id>comments</id>
	<name>Comments</name>
	<description>Files app plugin to add comments to files</description>
	<license-info>
	       <license>AGPL</license>
	       <licensee>Arthur Schiwon, Vincent Petry</licensee>
    </license-info>
	<default_enable/>
	<version>0.3.0</version>
	<dependencies>
		<owncloud min-version="10.0" max-version="10.0" />
	</dependencies>
	<types>
		<logging/>
		<dav/>
	</types>
	<sabre>
		<plugins>
			<plugin>OCA\Comments\Dav\CommentsPlugin</plugin>
		</plugins>
		<collections>
			<collection>OCA\Comments\Dav\RootCollection</collection>
		</collections>
	</sabre>
	<copyright>ownCloud, Inc</copyright>
</info>
